		"@wordpress/dom-ready": "2.6.0",
		"@wordpress/keycodes": "2.8.0",
		"@wordpress/scripts": "6.2.0",
		"autoprefixer": "9.7.4",
		"babel-plugin-import-glob": "2.0.0",
		"bootstrap": "4.4.1",
